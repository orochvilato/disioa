#depute-template {
  padding-top: 60px; }
  @media (max-width: 767px) {
    #depute-template {
      padding-top: 40px; } }
  #depute-template .tab-container header i {
    margin-right: 0.4em; }
  #depute-template .tab-content {
    display: none;
    padding: 0 5px 15px; }
    @media (max-width: 767px) {
      #depute-template .tab-content {
        font-size: 14px; } }
    #depute-template .tab-content h2 {
      border-bottom: 1px dashed #82cde2;
      margin: 50px 15px 25px;
      padding: 8px 15px; }
      @media (max-width: 767px) {
        #depute-template .tab-content h2 {
          font-size: 20px;
          margin-top: 30px; } }
    #depute-template .tab-content li {
      line-height: 1.6em; }
    #depute-template .tab-content a {
      color: #213558;
      text-decoration: underline; }
      #depute-template .tab-content a:hover {
        color: #82cde2; }

.depute-header {
  display: flex;
  margin-bottom: 60px; }
  @media (max-width: 767px) {
    .depute-header {
      display: block;
      margin-bottom: 40px; } }

.depute-photo {
  margin-top: 32px; }
  @media (max-width: 767px) {
    .depute-photo {
      margin: 0 auto;
      width: 40%; }
      .depute-photo img {
        width: 100%; } }

.depute-identite {
  margin: 0 15px; }
  .depute-identite header {
    color: white;
    margin: 0 -15px; }
    @media (max-width: 767px) {
      .depute-identite header {
        text-align: center; } }
  .depute-identite h2 span {
    background: #ff0052;
    display: inline-block;
    font-size: 16px;
    padding: 4px 6px; }
  .depute-identite h1 {
    background: #213558;
    display: inline-block;
    font-size: 24px;
    padding: 3px 5px; }

.depute-contacts {
  margin: 10px -5px 20px; }
  @media (max-width: 767px) {
    .depute-contacts {
      margin: 0 0 15px;
      text-align: center; } }
  .depute-contacts i {
    background: #82cde2;
    color: #fffefa;
    padding: 8px;
    transition: .2s; }
    .depute-contacts i:hover {
      background: #ff0052; }
  .depute-contacts .fa-facebook-square {
    background: #4867AA; }
  .depute-contacts .fa-twitter {
    background: #1DA1F2; }

.depute-groupe {
  font-size: 18px;
  margin-bottom: 15px; }
  @media (max-width: 767px) {
    .depute-groupe {
      font-size: 16px;
      margin-bottom: 10px; } }
  .depute-groupe a {
    color: #213558; }

.depute-infos {
  font-size: 15px;
  line-height: 1.5em; }
  @media (max-width: 767px) {
    .depute-infos {
      font-size: 13px; } }

.depute-hemicycle {
  margin: 50px auto 30px;
  width: 40%; }
  @media (max-width: 767px) {
    .depute-hemicycle {
      display: none; } }

.depute-watch .flex {
  align-items: flex-start; }
  @media (max-width: 767px) {
    .depute-watch .flex {
      padding-left: 10px; } }

.depute-watch img {
  margin-right: 15px; }
  @media (max-width: 767px) {
    .depute-watch img {
      display: none; } }

.depute-interventions-recap {
  font-size: 20px;
  font-weight: 600;
  margin: 60px 0 0;
  text-align: center; }
  .depute-interventions-recap span {
    color: #ff0052; }

.depute-interventions-cloud {
  display: flex;
  justify-content: space-around;
  margin-bottom: 50px; }
  @media (max-width: 1023px) {
    .depute-interventions-cloud {
      display: block; } }
  .depute-interventions-cloud > div {
    width: 48%; }
    @media (max-width: 1023px) {
      .depute-interventions-cloud > div {
        width: 100%; } }
  .depute-interventions-cloud .cloud {
    height: 250px;
    margin: auto;
    width: 90%; }

.depute-interventions-search {
  background: #97d5e7;
  margin: 0 15px 30px;
  padding: 6px 12px; }
  .depute-interventions-search i {
    color: white;
    width: 20px; }
  .depute-interventions-search input {
    background: transparent;
    border: 0;
    border-bottom: 1px solid rgba(255, 254, 250, 0.5);
    font-size: 15px;
    padding-bottom: 4px;
    width: calc(100% - 25px); }

/* GENERIC */
*,
*:before,
*:after {
  box-sizing: border-box;
  -moz-osx-font-smoothing: grayscale;
  -webkit-font-smoothing: antialiased; }

:focus {
  outline: 0; }

::-webkit-input-placeholder {
  /* Chrome/Opera/Safari */
  color: rgba(33, 53, 88, 0.75); }

::-moz-placeholder {
  /* Firefox 19+ */
  color: rgba(33, 53, 88, 0.75); }

:-ms-input-placeholder {
  /* IE 10+ */
  color: rgba(33, 53, 88, 0.75); }

:-moz-placeholder {
  /* Firefox 18- */
  color: rgba(33, 53, 88, 0.75); }

h1, h2, h3, h4,
p {
  margin: 0; }

a {
  text-decoration: none;
  transition: .2s; }
  a:hover {
    color: #82cde2;
    text-decoration: underline; }

input {
  color: #213558;
  font-family: Montserrat, sans-serif; }

.container {
  margin: auto;
  width: 980px; }
  @media (max-width: 1023px) {
    .container {
      width: 700px; } }
  @media (max-width: 767px) {
    .container {
      width: calc(100% - 20px); } }

.flex {
  display: flex; }

.tab-container {
  background: #fffefa;
  border: 1px solid #ffeda4;
  padding: 20px 15px; }
  @media (max-width: 767px) {
    .tab-container {
      padding: 10px 5px; } }
  .tab-container header {
    display: flex;
    margin: 0 -30px; }
    @media (max-width: 767px) {
      .tab-container header {
        margin: 0 -10px; } }
    .tab-container header input {
      display: none; }
    .tab-container header label {
      background: #213558;
      border: 1px solid #fffefa;
      color: white;
      cursor: pointer;
      display: block;
      font-weight: 600;
      padding: 12px 0;
      text-align: center;
      transition: .2s;
      width: calc(100% / 3); }
      @media (max-width: 767px) {
        .tab-container header label {
          padding: 8px 0; }
          .tab-container header label span {
            display: none; } }
    .tab-container header label:hover,
    .tab-container header input:checked + label {
      background: #ff0052; }
    .tab-container header i {
      margin-right: 0.4em; }

/* MAIN TEMPLATE */
body {
  background: #fffcf0;
  color: #213558;
  font-family: Montserrat, sans-serif;
  line-height: 1.5em;
  padding-bottom: 120px; }

#main-nav {
  background: #213558;
  height: 60px; }
  #main-nav img {
    height: 40px;
    margin: 10px 15px; }
  #main-nav ul {
    display: none;
    padding: 0 20px; }
    @media (max-width: 767px) {
      #main-nav ul {
        display: none; } }
  #main-nav li {
    display: inline-block;
    color: white;
    font-size: 13px;
    font-weight: 600;
    margin: 2px 20px 0 0;
    text-transform: uppercase; }

#header {
  background: #82cde2;
  font-size: 30px;
  font-weight: 700;
  height: 300px;
  line-height: 300px;
  text-align: center; }
  @media (max-width: 767px) {
    #header {
      font-size: 13px;
      height: 200px;
      line-height: 200px; } }

#sub-nav {
  background: #fffefa;
  border-bottom: 1px solid #ffeda4;
  height: 40px; }
  #sub-nav ul {
    padding: 0;
    text-align: center; }
  #sub-nav li {
    display: inline-block;
    font-size: 14px;
    margin-right: 20px; }
    @media (max-width: 767px) {
      #sub-nav li {
        font-size: 13px; } }
