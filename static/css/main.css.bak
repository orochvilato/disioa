/* GENERIC */
*,
*:before,
*:after {
  box-sizing: border-box;
  -moz-osx-font-smoothing: grayscale;
  -webkit-font-smoothing: antialiased; }

:focus {
  outline: 0; }

::-webkit-input-placeholder {
  /* Chrome/Opera/Safari */
  color: rgba(33, 53, 88, 0.75); }

::-moz-placeholder {
  /* Firefox 19+ */
  color: rgba(33, 53, 88, 0.75); }

:-ms-input-placeholder {
  /* IE 10+ */
  color: rgba(33, 53, 88, 0.75); }

:-moz-placeholder {
  /* Firefox 18- */
  color: rgba(33, 53, 88, 0.75); }

h1, h2, h3, h4,
p {
  margin: 0; }

a {
  text-decoration: none;
  transition: .2s; }
  a:hover {
    color: #82cde2;
    text-decoration: underline; }

input {
  color: #213558;
  font-family: Montserrat, sans-serif; }

.container {
  margin: auto;
  width: 980px; }
  @media (max-width: 1023px) {
    .container {
      width: 700px; } }
  @media (max-width: 767px) {
    .container {
      width: calc(100% - 20px); } }

.flex {
  display: flex; }

.tab-container {
  background: #fffefa;
  border: 1px solid #ffeda4;
  padding: 20px 15px;
  position: relative; }
  @media (max-width: 767px) {
    .tab-container {
      padding: 10px 5px; } }
  .tab-container > header {
    display: flex;
    margin: 0 -30px; }
    @media (max-width: 767px) {
      .tab-container > header {
        margin: 0 -10px; } }
    .tab-container > header input {
      display: none; }
    .tab-container > header label {
      background: #213558;
      border: 1px solid #fffefa;
      color: white;
      cursor: pointer;
      display: block;
      font-weight: 600;
      padding: 12px 0;
      text-align: center;
      transition: .2s;
      width: calc(100% / 3); }
      @media (max-width: 767px) {
        .tab-container > header label {
          padding: 8px 0; }
          .tab-container > header label span {
            display: none; } }
    .tab-container > header label:hover,
    .tab-container > header input:checked + label {
      background: #ff0052; }
    .tab-container > header i {
      margin-right: 0.4em; }

.tab-content {
  height: 0;
  opacity: 0;
  overflow: hidden;
  position: absolute;
  top: 0;
  transform: translateY(-20px);
  transition: .2s;
  width: 100%;
  z-index: -1; }
  .tab-content.visible {
    height: auto;
    opacity: 1;
    position: static;
    transform: translateY(0); }

